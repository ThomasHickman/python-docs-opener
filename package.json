{
  "name": "python-docs-fetcher",
  "displayName": "python-docs-fetcher",
  "description": "",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:python-docs-fetcher.getDocsOfSymbol"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-docs-fetcher.getDocsOfSymbol",
        "title": "Python Doc Fetcher: Open documentation of symbol in a new browser window"
      }
    ],
    "keybindings": [
      {
        "command": "python-docs-fetcher.getDocsOfSymbol",
        "key": "shift+f1",
        "mac": "shift+f1",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Python Help Fetcher",
      "properties": {
        "python-docs-fetcher.additionalLibraryDocsMappings": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "Specifies additional mappings from libraries to the online documentation. You can use the variables `${symbolName}` and TODO: fill this in"
        }
      }
    }
  },
  "extensionKind": [
    "workspace"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
